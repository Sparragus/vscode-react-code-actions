{
  "name": "vscode-react-code-actions",
  "displayName": "React Code Actions",
  "description": "React Code Actions for VS Code",
  "license": "MIT",
  "version": "0.0.1",
  "author": {
    "name": "Richard B. Kaufman LÃ³pez",
    "email": "richardbkaufman@gmail.com",
    "url": "https://richardkaufman.dev"
  },
  "homepage": "https://github.com/Sparragus/vscode-react-code-actions#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sparragus/vscode-react-code-actions.git"
  },
  "bugs": {
    "url": "https://github.com/Sparragus/vscode-react-code-actions/issues"
  },
  "keywords": [
    "react",
    "code action",
    "refactor"
  ],
  "categories": [
    "Formatters",
    "Programming Languages",
    "Other",
    "Snippets"
  ],
  "engines": {
    "vscode": "^1.48.0"
  },
  "publisher": "sparragus",
  "icon": "assets/icon.png",
  "activationEvents": [
    "workspaceContains:package.json"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "react-code-actions.extractToFunction",
        "title": "Extract to function component in same file"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/babel__template": "^7.0.2",
    "@types/babel__traverse": "^7.0.13",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/vscode": "^1.48.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "glob": "^7.1.6",
    "mocha": "^8.0.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "@babel/parser": "^7.11.4",
    "@babel/template": "^7.10.4",
    "@babel/traverse": "^7.11.0",
    "@babel/types": "^7.11.0"
  }
}
